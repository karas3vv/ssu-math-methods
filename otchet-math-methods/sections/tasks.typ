import numpy as np
import pandas as pd 

def f(x, y, V=5):
    return 2 * V * x + V * x**2 - y

# –ú–µ—Ç–æ–¥ –≠–π–ª–µ—Ä–∞ 
def euler_method(x0, y0, h, n, V):
    x = np.zeros(n + 1)
    y = np.zeros(n + 1)
    x[0] = x0
    y[0] = y0

    for i in range(n):
        x[i + 1] = x[i] + h
        y[i + 1] = y[i] + h * f(x[i], y[i], V)
    
    return x, y

# –£—Å–æ–≤–µ—Ä—à–µ–Ω—Å—Ç–≤–æ–≤–∞–Ω–Ω—ã–π –º–µ—Ç–æ–¥ –≠–π–ª–µ—Ä–∞ 
def improved_euler_method(x0, y0, h, n, V):
    x = np.zeros(n + 1)
    y = np.zeros(n + 1)
    x[0] = x0
    y[0] = y0 

    for i in range(n):
        x[i + 1] = x[i] + h
        y_half = y[i] + (h / 2) * f(x[i], y[i], V)
        x_half = x[i] + h / 2
        y[i + 1] = y[i] + h * f(x_half, y_half, V)

    return x, y

def exact_solution(x, V=5):
    return V * x**2

